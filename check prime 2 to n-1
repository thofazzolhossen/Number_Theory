#include<iostream>
using namespace std;


bool prime(int x){
    for(int i=2;i<x;i++){
        if(x%i==0){
            cout<<x<<" % "<<i<<" ";
            return false;
        }
    }
    return true;
}




int main(){
    int t;cin>>t;
    bool ans = prime(t);
    if(ans==true) cout<<"It is prime";
    else cout<<"It is not prime";



return 0;
}
